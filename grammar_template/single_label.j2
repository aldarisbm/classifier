{# grammar_template/single_label.j2 #}
root ::= Label
Labels ::= "\"Technology\"" | "\"Healthcare\"" | "\"Finance\"" | "\"Manufacturing\"" | "\"Retail\"" | "\"Education\"" | "\"Energy\"" | "\"Agriculture\"" | "\"Other\""
Label ::= "{"   ws   "\"label\":"   ws   Labels   "}"
Labellist ::= "[]" | "["   ws   Label   (","   ws   Label)*   "]"
string ::= "\""   ([^"]*)   "\""
boolean ::= "true" | "false"
ws ::= [ \t\n]*
number ::= [0-9]+   "."?   [0-9]*
stringlist ::= "["   ws   "]" | "["   ws   string   (","   ws   string)*   ws   "]"
numberlist ::= "["   ws   "]" | "["   ws   string   (","   ws   number)*   ws   "]"
