{# grammar_templates/multi_label.j2 #}
{% set formatted_labels = [] %}
{% for label in labels %}
    {% set formatted_label = "\"\\\"" + label + "\\\"\"" %}
    {% if not loop.last %}
        {% set formatted_label = formatted_label + " |" %}
    {% endif %}
    {% set _ = formatted_labels.append(formatted_label) %}
{% endfor %}
root ::= Label
Label ::= {{ formatted_labels|join(" ") }}
