{# grammar_template/multi_label.j2 #}
{% set formatted_labels = [] %}
{% for label in labels %}
    {% set formatted_label = "\"\\\"" + label + "\\\"\"" %}
    {% if not loop.last %}
        {% set formatted_label = formatted_label + " |" %}
    {% endif %}
    {% set _ = formatted_labels.append(formatted_label) %}
{% endfor %}
root ::= Label
Labels ::= {{ formatted_labels|join(" ") }}
Label ::= "{"   ws   "\"labels\":"   ws   Labelslist   "}"
Labellist ::= "[]" | "["   ws   Label   (","   ws   Label)*   "]"
string ::= "\""   ([^"]*)   "\""
boolean ::= "true" | "false"
ws ::= [ \t\n]*
number ::= [0-9]+   "."?   [0-9]*
stringlist ::= "["   ws   "]" | "["   ws   string   (","   ws   string)*   ws   "]"
numberlist ::= "["   ws   "]" | "["   ws   string   (","   ws   number)*   ws   "]"
